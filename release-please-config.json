{
  "include-component-in-tag": false,
  "separate-pull-requests": false,
  "release-type": "node",
  "packages": {
    ".": {
      "path": ".",
      "release-type": "node",
      "package-name": "ratings-aggregator"
    },
    "frontend": {
      "path": "frontend",
      "release-type": "node",
      "package-name": "ratings-pro-frontend"
    }
  },
  "plugins": ["linked-versions"],
  "packages/plugins": {
    ".": { "type": "node-workspace", "updateAllPackages": true }
  }
}
